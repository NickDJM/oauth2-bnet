{
    "name": "nickdjm/oauth2-bnet",
    "description": "Battle.net OAuth 2.0 Client Provider for The PHP League OAuth2-Client.",
    "require": {
        "league/oauth2-client": "^2.0",
        "php": "^8.0",
        "ext-json": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nickdjm\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nickdjm\\OAuth2\\Client\\Tests\\": "test/src/"
        }
    },
    "authors": [
        {
            "name": "Nick Milton",
            "email": "nickdjm@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^11",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "scripts": {
      "phpcs": "phpcs src test --standard=PSR2 -sp",
      "phpcbf": "phpcbf src test --standard=PSR2 -sp"
    }
}
